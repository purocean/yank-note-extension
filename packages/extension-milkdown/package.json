{
  "name": "@yank-note/extension-milkdown",
  "version": "1.1.1",
  "displayName": "Milkdown",
  "description": "WYSIWYG Markdown Editor",
  "displayName_ZH-CN": "Milkdown",
  "description_ZH-CN": "所见即所得的 Markdown 编辑器",
  "author": "Yank Note",
  "license": "AGPL-3.0",
  "keywords": [],
  "homepage": "https://github.com/purocean/yank-note-extension/tree/main/packages/extension-milkdown",
  "repository": {
    "type": "git",
    "url": "https://github.com/purocean/yank-note-extension.git"
  },
  "main": "./main/index.js",
  "style": "./main/style.css",
  "icon": "./icon.svg",
  "engines": {
    "node": ">=14.6.0",
    "yank-note": "^3.43.0"
  },
  "requirements": {
    "premium": true
  },
  "files": [
    "dist",
    "main",
    "editor",
    "themes",
    "*.md",
    "icon.svg"
  ],
  "scripts": {
    "link-extension": "link-yn-extension",
    "unlink-extension": "link-yn-extension --unlink",
    "dev:main": "vite build --mode main --watch",
    "dev:editor": "vite build --mode editor --watch",
    "build:main": "vue-tsc --noEmit && vite build --mode main",
    "build:editor": "vue-tsc --noEmit && vite build --mode editor",
    "build": "npm run build:main && npm run build:editor",
    "lint": "vue-tsc --noEmit && yarn run eslint '{src,__tests__}/**/*.{js,ts,tsx,vue}'",
    "version": "conventional-changelog -t extension-milkdown- --commit-path . -p angular -i CHANGELOG.md -s && npm run tag-version",
    "tag-version": "git add . && git commit -m 'chore(release): bump extension-milkdown version' && git tag extension-milkdown-$npm_package_version",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "@fontsource/roboto": "^4.5.8",
    "@milkdown/core": "^6.5.0",
    "@milkdown/plugin-block": "^6.5.0",
    "@milkdown/plugin-diagram": "^6.5.0",
    "@milkdown/plugin-emoji": "^6.5.1",
    "@milkdown/plugin-history": "^6.5.0",
    "@milkdown/plugin-indent": "^6.5.0",
    "@milkdown/plugin-listener": "^6.5.0",
    "@milkdown/plugin-math": "^6.5.2",
    "@milkdown/plugin-menu": "^6.5.0",
    "@milkdown/plugin-prism": "^6.5.0",
    "@milkdown/plugin-slash": "^6.5.2",
    "@milkdown/plugin-tooltip": "^6.5.0",
    "@milkdown/plugin-trailing": "^6.5.0",
    "@milkdown/plugin-upload": "^6.5.2",
    "@milkdown/preset-commonmark": "^6.5.1",
    "@milkdown/preset-gfm": "^6.5.0",
    "@milkdown/prose": "^6.5.0",
    "@milkdown/theme-nord": "^6.5.0",
    "@milkdown/theme-pack-helper": "^6.5.2",
    "@milkdown/utils": "^6.5.0",
    "@rollup/plugin-node-resolve": "^13.2.1",
    "@rollup/plugin-replace": "^4.0.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/eslint-config-standard": "^6.1.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@yank-note/runtime-api": "workspace:^3.56.2",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "katex": "^0.16.3",
    "material-icons": "^1.12.1",
    "prism-themes": "^1.9.0",
    "refractor": "^4.8.0",
    "remark-frontmatter": "^4.0.1",
    "typescript": "^4.6.4",
    "vite": "^3.2.3",
    "vue": "^3.2.33",
    "vue-tsc": "^0.34.11"
  }
}
