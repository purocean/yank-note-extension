{
  "name": "@yank-note/extension-openai",
  "version": "1.0.5",
  "displayName": "OpenAI",
  "description": "Your AI writing assistant",
  "displayName_ZH-CN": "OpenAI",
  "description_ZH-CN": "你的人工智能写作助手",
  "author": "Yank Note",
  "license": "AGPL-3.0",
  "keywords": [],
  "homepage": "https://github.com/purocean/yank-note-extension/tree/main/packages/extension-openai",
  "repository": {
    "type": "git",
    "url": "https://github.com/purocean/yank-note-extension.git"
  },
  "main": "./dist/index.js",
  "icon": "./icon.svg",
  "engines": {
    "node": ">=14.6.0",
    "yank-note": "^3.29.0"
  },
  "files": [
    "dist",
    "themes",
    "*.md",
    "icon.svg"
  ],
  "scripts": {
    "link-extension": "link-yn-extension",
    "unlink-extension": "link-yn-extension --unlink",
    "dev": "vite build --watch",
    "build": "vue-tsc --noEmit && vite build",
    "lint": "vue-tsc --noEmit && yarn run eslint '{src,__tests__}/**/*.{js,ts,tsx,vue}'",
    "version": "conventional-changelog -t extension-openai- --commit-path . -p angular -i CHANGELOG.md -s && npm run tag-version",
    "tag-version": "git add . && git commit -m 'chore(release): bump extension-openai version' && git tag extension-openai-$npm_package_version",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.2.1",
    "@rollup/plugin-replace": "^4.0.0",
    "@vitejs/plugin-vue": "^2.3.2",
    "@vue/eslint-config-standard": "^6.1.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@yank-note/runtime-api": "workspace:^3.33.0",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "typescript": "^4.6.4",
    "vite": "^2.9.8",
    "vue": "^3.2.33",
    "vue-tsc": "^0.34.11"
  }
}
