  "description_ZH-CN": "HelloWorld 示例扩展",
  "author": "Yank Note",
  "license": "MIT",
  "keywords": [],
  "homepage": "https://github.com/purocean/yn",
  "main": "./dist/index.js",
  "style": "./dist/style.css",
  "icon": "./icon.png",
  "engines": {
    "node": ">=18.20.2",
    "yank-note": ">=3.79.1"
  },
  "themes": [
    {
      "name": "Red",
      "css": "./themes/demo1.css"
    },
    {
      "name": "Green",
      "css": "./themes/demo2.css"
    }
  ],
  "files": [
    "*.md",
    "dist",
    "themes",
    "icon.png"
  ],
  "scripts": {
    "link-extension": "link-yn-extension",
